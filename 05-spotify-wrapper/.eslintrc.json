{
    "env": {
        "browser": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-unused-expressions": "off",
      "object-curly-newline": "off",
      "operator-linebreak": "off"
    }
}
