{
    "env": {
        "browser": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-unused-expressions": "off",
      "no-unused-vars": ["warn", {"varsIgnorePattern": "^_$", "argsIgnorePattern": "^_$"}],
      "object-curly-newline": "off"
    }
}
